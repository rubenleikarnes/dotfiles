{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "active_pane_magnification": 1.0,
  "auto_update": true,
  "autosave": "on_focus_change",
  "base_keymap": "SublimeText",
  "buffer_font_family": "JetBrainsMonoNL Nerd Font",
  "buffer_font_features": {
    "calt": true
  },
  "buffer_font_size": 15,
  "buffer_line_height": "comfortable",
  "chat_panel": {
    "button": false
  },
  "confirm_quit": true,
  "cursor_blink": false,
  "cursor_shape": "bar",
  "current_line_highlight": "all",
  "collaboration_panel": {
    "button": false
  },
  "default_dock_anchor": "bottom",
  "enable_language_server": true,
  "features": {
    "copilot": false
  },
  "file_types": {
    "HTML": ["svg", "njk"]
  },
  "format_on_save": "off",
  "formatter": "auto",
  "git": {
    "git_gutter": "tracked_files"
  },
  "gutter": {
    "line_numbers": true,
    "code_actions": true,
    "folds": true
  },
  "indent_guides": {
    "active_line_width": 4,
    "enabled": true,
    "line_width": 2,
    "coloring": "indent_aware",
    "background_coloring": "disabled"
  },
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true
  },
  "hard_tabs": false,
  "languages": {
    "JSON": {
      "hard_tabs": false,
      "tab_size": 2
    }
  },
  "line_indicator_format": "long",
  "preferred_line_length": 100,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "project_panel": {
    "dock": "left",
    "git_status": true,
    "default_width": 400,
    "file_icons": true,
    "folder_icons": true,
    "indent_size": 25,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "scrollbar": {
      "show": "always"
    }
  },
  "project_panel.auto_fold_dirs": true,
  "tab_bar": {
    "show_nav_history_buttons": false
  },
  "tab_size": 2,
  "tabs": {
    "git_status": true,
    "close_position": "right"
  },
  "theme": "XY-Zed",
  "experimental.theme_overrides": {
    "editor.active_line.background": "#7de48620",
    "search.match_background": "#db5cc360",
    /// Read-access of a symbol, like reading a variable.
    ///
    /// A document highlight is a range inside a text document which deserves
    /// special attention. Usually a document highlight is visualized by changing
    /// the background color of its range.
    "editor.document_highlight.read_background": "#9bdae90f",
    /// Read-access of a symbol, like reading a variable.
    ///
    /// A document highlight is a range inside a text document which deserves
    /// special attention. Usually a document highlight is visualized by changing
    /// the background color of its range.
    "editor.document_highlight.bracket_background": "#7de48650",
    /// Highlighted brackets background color.
    ///
    /// Matching brackets in the cursor scope are highlighted with this background color.
    "editor.document_highlight.write_background": "#7de486f7",
    "editor.": "ff00ffff",
    "players": [
      {
        "cursor": "#ffffffff",
        "background": "#7de486ff",
        "selection": "#7de4863d"
      }
    ]
  },
  "scroll_beyond_last_line": "one_page",
  "scrollbar": {
    "show": "always",
    "git_diff": true,
    "selections": true,
    "symbols_selections": true,
    "diagnostics": true
  },
  "show_inline_completions": true,
  "show_call_status_icon": true,
  "show_whitespaces": "all",
  "soft_wrap": "editor_width",
  "ui_font_family": "Inter",
  "ui_font_features": {
    "calt": true
  },
  "ui_font_size": 15,
  "use_autoclose": true,
  "vertical_scroll_margin": 15
}
